{
  "name": "@twilio/websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for Twilio real-time features",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc --build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"tsc --build && node dist/server.js\"",
    "dev:phone": "nodemon --watch src --ext ts --exec \"tsc --build && node dist/dev-phone-server.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@twilio/shared": "file:../shared",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "next": "15.1.7",
    "openai": "^4.85.1",
    "twilio": "^5.4.4",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "nodemon": "3.1.9",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
